#include<bits/stdc++.h>
 
using namespace std;
 
int main()
{
	string s;
	cin >> s;
	int c1 =0, c2 =0;
	for(auto i:s)
	{
		int temp = i - 'A';
		if(temp < 26)
		c1++;
		else
		c2++;
	}
	
	if(c1 > c2)
	{
		for(auto &i:s)
		{
			int temp = i - 'A';
			
			if(temp > 26)
			i-= 32;
		}
	}
	else
	{
		for(auto &i:s)
		{
			int temp = i - 'A';
			
			if(temp <26)
			i+= 32;
		}
	}
	cout << s;
	return 0;
}
