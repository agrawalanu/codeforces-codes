#include<bits/stdc++.h>

using namespace std;

int main()
{
	int n,k;
	cin >> n>> k;
	
	if(k ==0)
	{
		for(int  i =1; i<=n; i++)
		cout << i << " ";
	}
	else
	{
		
		for(int i =1; i<=k; i++)
		{
			cout << n-i+1 << " ";
			 	
		}
		for(int j = 1; j<=n-k; j++)
			cout << j<< " ";	
		
	}
	return 0;
}
