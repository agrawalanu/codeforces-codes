#include<bits/stdc++.h>
#define int long long
using namespace std;

int32_t main()
{
	 int l , r;
	 cin >>l >> r;
	 
	 if(r-l <=1 || (r-l == 2 && l%2 !=0))
	 	cout << -1;
	else
	{
		if(l%2 == 0)
			cout << l << " " << l+1 << " " << l+2;
		else
			cout << l+1 << " " << l+2 << " " << l+3;
	}
	return 0;
}
